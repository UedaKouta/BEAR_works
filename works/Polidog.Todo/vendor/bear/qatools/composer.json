{
    "name": "bear/qatools",
    "description": "PHP QA Tools",
    "keywords": [
        "php",
        "qa"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "friendsofphp/php-cs-fixer": "^2.16",
        "mayflower/php-codebrowser": "^2.0",
        "phploc/phploc": "^5.0",
        "phpmd/phpmd": "^2.7",
        "phpmetrics/phpmetrics": "^2.4",
        "phpstan/phpstan": "~0.11",
        "phpunit/phpunit": "^7.5|^8.5|^9.1",
        "psalm/plugin-phpunit": "~0.10",
        "sebastian/phpcpd": "^3.0 || ^4.1",
        "squizlabs/php_codesniffer": "^3.5",
        "vimeo/psalm": "^3.6"
    },
    "bin": [
        "bin/phpbuild",
        "bin/phptest"
    ],
    "config": {
        "sort-packages": true
    }
}
